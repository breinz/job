{
  "name": "games",
  "version": "1.0.0",
  "description": "Personal site",
  "main": "dist/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch-ts": "tsc -w",
    "watch-node": "NODE_ENV=development nodemon",
    "watch-sass": "node-sass src/scss/main.scss public/css/main.css -w src/scss --include-path node_modules/foundation-sites/scss/",
    "mongo": "cd ~/Developer; mongod --dbpath=mongodb",
    "pack": "watchify dist/games/pack/index.js -o public/js/games/pack.js -v",
    "amaze": "watchify dist/games/amaze/index.js -o public/js/games/amaze.js -v",
    "script": "watchify dist/script/script.js -o public/js/script.js -v",
    "w": "concurrently -k -p '[{name}]' -n 'TypScript,Node,Sass,Js' -c 'yellow.bold,cyan.bold,blue.bold,red.bold' 'npm run watch-ts' 'npm run watch-node' 'npm run watch-sass' 'npm run script'",
    "watch-save": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node,Sass,Browser,Admin\" -c \"yellow.bold,cyan.bold,blue.bold,green.bold,orange.bold\" \"npm run watch-ts\" \"npm run watch-node\" \"npm run watch-sass\" \"npm run watch-browser\" \"npm run watch-admin\""
  },
  "author": "Julien Breiner",
  "license": "ISC",
  "sharp-version-used": "0.21.2",
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@types/gsap": "^1.20.0",
    "babelify": "^10.0.0",
    "bcrypt-nodejs": "0.0.3",
    "change-case": "^3.1.0",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.4",
    "express-fileupload": "^1.1.1-alpha.1",
    "express-session": "^1.15.6",
    "foundation-sites": "^6.5.1",
    "gsap": "^2.0.2",
    "iplocation": "^6.1.0",
    "jquery": "^3.3.1",
    "mongoose": "^5.3.14",
    "pixi.js": "^4.8.2",
    "pug": "^2.0.3",
    "sharp": "^0.17",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/change-case": "^2.3.1",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.16.0",
    "@types/express-fileupload": "^0.4.0",
    "@types/express-session": "^1.15.11",
    "@types/foundation-sites": "^6.4.3",
    "@types/jquery": "^3.3.29",
    "@types/mongoose": "^5.3.1",
    "@types/pixi.js": "^4.8.4",
    "@types/sharp": "^0.21.0"
  }
}
